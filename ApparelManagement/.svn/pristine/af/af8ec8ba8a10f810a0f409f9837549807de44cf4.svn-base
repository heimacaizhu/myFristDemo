package com.clothes.pojo;

public class Orderitem {
	//订单条目id
    private String oiId;
    //所属订单
    private Orders orders;
    //商品明细
    private Goodstitem goodsItem;
    //数量
    private Integer num;
    //状态
    private Integer oiState;

    public String getOiId() {
        return oiId;
    }

    public void setOiId(String oiId) {
        this.oiId = oiId;
    }
    
    public Integer getNum() {
        return num;
    }

    public void setNum(Integer num) {
        this.num = num;
    }

    public Integer getOiState() {
        return oiState;
    }

    public void setOiState(Integer oiState) {
        this.oiState = oiState;
    }

	public Orders getOrders() {
		return orders;
	}

	public void setOrders(Orders orders) {
		this.orders = orders;
	}

	public Goodstitem getGoodsItem() {
		return goodsItem;
	}

	public void setGoodsItem(Goodstitem goodsItem) {
		this.goodsItem = goodsItem;
	}

	@Override
	public String toString() {
		return "Orderitem [oiId=" + oiId + ", orders=" + orders + ", goodsItem=" + goodsItem + ", num=" + num
				+ ", oiState=" + oiState + "]";
	}
	
}
