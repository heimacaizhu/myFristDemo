package com.clothes.pojo;

/**
 * 物品出入库记录明细实体类
 * @author 项目组
 *
 */
public class Recorditem {
	//id
    private String riId;
    //所属出入库记录
    private Record record;
    //物品明细
    private Goodstitem goodsitem;
    //物品出入库数量
    private Integer num;
    //状态
    private Integer riState;

    public String getRiId() {
        return riId;
    }

    public void setRiId(String riId) {
        this.riId = riId;
    }

    public Integer getNum() {
        return num;
    }

    public void setNum(Integer num) {
        this.num = num;
    }

    public Integer getRiState() {
        return riState;
    }

    public void setRiState(Integer riState) {
        this.riState = riState;
    }

	public Record getRecord() {
		return record;
	}

	public void setRecord(Record record) {
		this.record = record;
	}

	public Goodstitem getGoodsitem() {
		return goodsitem;
	}

	public void setGoodsitem(Goodstitem goodsitem) {
		this.goodsitem = goodsitem;
	}

	@Override
	public String toString() {
		return "Recorditem [riId=" + riId + ", record=" + record + ", goodsitem=" + goodsitem + ", num=" + num
				+ ", riState=" + riState + "]";
	}
    
}
