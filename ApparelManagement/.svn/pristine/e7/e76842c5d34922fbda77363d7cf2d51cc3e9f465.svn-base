package com.clothes.pojo;

/**
 * 物品库存实体类
 * @author 项目组
 *
 */
public class Inventory {
	//id
    private int invenId;
    //物品明细
    private Goodstitem goodsItem;
    //仓库
    private Store store;
    //库存数量
    private Integer num;
    //最小库存数量
    private Integer minnum;
    //状态
    private Integer invenState;

    public int getInvenId() {
        return invenId;
    }

    public void setInvenId(int invenId) {
        this.invenId = invenId;
    }

    public Integer getNum() {
        return num;
    }

    public void setNum(Integer num) {
        this.num = num;
    }

    public Integer getMinnum() {
        return minnum;
    }

    public void setMinnum(Integer minnum) {
        this.minnum = minnum;
    }

    public Integer getInvenState() {
        return invenState;
    }

    public void setInvenState(Integer invenState) {
        this.invenState = invenState;
    }

	public Store getStore() {
		return store;
	}

	public void setStore(Store store) {
		this.store = store;
	}

	public Goodstitem getGoodsItem() {
		return goodsItem;
	}

	public void setGoodsItem(Goodstitem goodsItem) {
		this.goodsItem = goodsItem;
	}

	@Override
	public String toString() {
		return "Inventory [invenId=" + invenId + ", goodsItem=" + goodsItem + ", store=" + store + ", num=" + num
				+ ", minnum=" + minnum + ", invenState=" + invenState + ", getInvenId()=" + getInvenId() + ", getNum()="
				+ getNum() + ", getMinnum()=" + getMinnum() + ", getInvenState()=" + getInvenState() + ", getStore()="
				+ getStore() + ", getGoodsItem()=" + getGoodsItem() + ", getClass()=" + getClass() + ", hashCode()="
				+ hashCode() + ", toString()=" + super.toString() + "]";
	}
    
}
