$(document).ready(function(){
	//客户管理菜单部分点击
	khNavClick();
});

/*
 * 客户管理菜单点击
 */
function khNavClick(){
	//加载添加客户
	$(".kh-dd1").click(function(){
		$(".layui-tab-content").load("kh-add.html");
	});
	//客户区域
	$(".region-show").click(function(){
		$(".layui-tab-content").load("region.html");
	});
	//加载客户页面
	$(".kh-show").click(function(){
		$(".layui-tab-content").load("kh-show.html");
	});
	//客户统计
	$(".kh-count").click(function(){
		$(".layui-tab-content").load("kh-count.html");
	});
	//搜索商品
	$(".goods-s").click(function(){
		$(".layui-tab-content").load("goods-search.html");
	});
	//加载客服报单页面
	$(".order-kf").click(function(){
		$(".layui-tab-content").load("order-kf.html");
	});
}
